<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="15" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ClubRentalController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Text style="-fx-font-size: 20; -fx-font-weight: bold;" text="Rent a Set of Golf Clubs" />
    <ListView fx:id="clubListView" prefHeight="200" prefWidth="300" />
    <TextField fx:id="rentalQuantityField" promptText="Enter quantity to rent" />
    <Button onAction="#rentClub" text="Rent Selected Club" />
</VBox>
